generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int        @id @default(autoincrement())
  name      String
  email     String     @unique
  password  String
  phone     String?
  addresses Address[]
  orders    Order[]
  cartItems CartItem[]
  reviews   Review[]
  createdAt DateTime   @default(now())
}